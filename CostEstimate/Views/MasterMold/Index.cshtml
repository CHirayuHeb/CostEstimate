@model CostEstimate.Models.Common.Class;
@using CostEstimate.Models.Common;
@{
    ViewData["Title"] = "Home | Thai Stanley";
    Layout = "~/Views/Shared/_MenuBar_mold.cshtml";
}
@{
    string _Permission = User.Claims.FirstOrDefault(s => s.Type == "Permission").Value;
    string _NickName = User.Claims.FirstOrDefault(s => s.Type == "NICKNAME")?.Value;
    string _Name = User.Claims.FirstOrDefault(s => s.Type == "Name")?.Value;
    string _SurName = User.Claims.FirstOrDefault(s => s.Type == "SurName")?.Value;
    string _Division = User.Claims.FirstOrDefault(s => s.Type == "Division")?.Value;
    string _Section = User.Claims.FirstOrDefault(s => s.Type == "Section")?.Value;
    string _admin = GlobalVariable.perAdmin.ToUpper();
}
<link href="~/css/Home/siteHome.css" rel="stylesheet" />
<link href="~/css/Home/siteHome.css" rel="stylesheet" />
<script type="text/javascript">document.onreadystatechange = () => { if (document.readyState === "interactive") { PositionY("MasterMold"); } }</script>
<div class="home-content">
    <div id="DisplayContent" class="content-box">
        @*<div class="white-space"></div>*@
        <div style="margin:2%" class="content-text">
            <div class="grid-item fw-800" style="text-align:left;font-size:24px;padding-top:0px"><span style="color:darkorange">MASTER</span> MOLD.</div>
            <br />
            @*<div style="min-height: 600px;max-height:600px;overflow-y: auto;padding:0px;border:0px solid white;scrollbar-color: #c9c3f0 white;">*@
            <div style="width:100%">
                @*<div onclick="window.open('newurl.html','mywindow');" style="cursor: pointer;">&nbsp;</div>*@
                <div class="centerdiv" id="divHomeProduct" style="margin-inline: auto;" onclick="GoSideMenuNewRequest('AddCost')">
                    <div class="row ">
                        <div class="col-20" style=" display: flex;align-items: center; ">
                            <img src="~/images/planning_1.png" style="max-height:100px; max-width:100px;" class="center" />
                        </div>

                        <div class="col-80">
                            <div class="grid-item fw-800" style="text-align:left;">COST</div>
                            <div class="grid-item fw-600" style="text-align:left;font-size:10px;">1.COST PLANNING</div>
                            <div class="grid-item fw-600" style="text-align:left;font-size:10px;">2.MODEL PLANNING</div>
                        </div>

                    </div>


                </div>
                <div class="centerdiv" id="divHomeMold" style="margin-inline: auto;" onclick="GoSideMenuNewRequest('AddProcess')">
                    <div class="row ">
                        <div class="col-20" style=" display: flex;align-items: center; ">
                            <img src="~/images/process.png" style="max-height:100px; max-width:100px;" class="center" />
                        </div>
                        <div class="col-80">
                            <div class="grid-item fw-800" style="text-align:left;">MASTER PROCESS</div>
                            <div class="grid-item fw-600" style="text-align:left;font-size:10px;">1.MASTER PROCESS SHEET DETAIL</div>
                        </div>

                    </div>


                </div>
                <div class="centerdiv" id="divHomeMold" style="margin-inline: auto;" onclick="GoSideMenuNewRequest('AddModel')">
                    <div class="row ">
                        <div class="col-20" style=" display: flex;align-items: center; ">
                            <img src="~/images/transaction-history.png" style="max-height:100px; max-width:100px;" class="center" />
                        </div>
                        <div class="col-80">
                            <div class="grid-item fw-800" style="text-align:left;">MASTER MODEL</div>
                            <div class="grid-item fw-600" style="text-align:left;font-size:10px;">1.MASTER MODEL SHEET DETAIL</div>
                        </div>

                    </div>


                </div>
            </div>


        </div>
    </div>
</div>